Algoritmo "idade"

Var
     a, b, troca : inteiro
      


Inicio
       escreva("digite o primeiro numero")
       leia(a)
       escreva("digite o segundo numero")
       leia(b)
       
       troca <- a
       a <- b
       b <- troca
       
       escreval("o novo valor de A é",a)
       escreval("o novo valor de B é", b)



Fimalgoritmo